# EC2 instance variable

ec2:
  region: us-east-2c
  credentials: newkeys
  image: ami-0f1a02d93feff123e
  sg: AllowSSH
  instance_type: t2.micro
  subnet: subnet-ed9001b5
  public_ip: yes
  tags:
    Name: Ec2Ansible
  volumes:
    - device_name: /dev/xvda
      volume_type: standard
      volume_size: 8
  



